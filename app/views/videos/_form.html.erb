<% form_for @video do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :location %><br />
    <%= f.text_field :location %>
  </p>
  <p>
    <%= f.label :date_of_event %><br />
    <%= f.datetime_select :date_of_event %>
  </p>
  <p>
    <%= f.label :emphasis %><br />
    <%= f.text_field :emphasis %>
  </p>
  <p>
    <%= f.label :cameraman %><br />
    <%= f.text_field :cameraman %>
  </p>
  <p>
    <%= f.label :information %><br />
    <%= f.text_area :information %>
  </p>
  <p>
    <%= f.label :embed %><br />
    <%= f.text_area :embed %>
  </p>
  <p>
    <%= f.label :identity %><br />
    <%= f.text_field :identity %>
  </p>

  <p><%= f.submit %></p>
<% end %>

